int main() {
    int b = 2;
    int e = 4;
    int r = 1;
    while (e > 0)
        //@loop_invariant e >= 0
        // r * b^e remains invariant
    {
        if (e % 2 != 0)
            r = r * b;
        b = b * b;
        e = e / 2;
    }

    return r;
}
